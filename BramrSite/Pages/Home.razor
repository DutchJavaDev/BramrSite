                <div class="form-container sign-up-container">
                    <div class="sign-in-form">
                        <EditForm Model="Model" OnValidSubmit="SignUpSubmit">
                            <DataAnnotationsValidator />
                            <ValidationSummary Model="Model" />

                            <h1 class="text-center">Register</h1>

                            <div class="w-75 m-auto">
                                <input type="email" @bind-value="Model.Email" placeholder="Email" />
                                <input type="text" @bind-value="Model.UserName" placeholder="Username" />
                            </div>

                            <div>
                                <p>@SignUpMessage</p>
                            </div>

                            <div class="w-75 m-auto">
                                @if (Disabled)
                                {
                                    <button id="signUp-panel" class="btn w-75 btn-primary" disabled="@Disabled" type="submit">
                                        <div class="spinner-border" role="status">
                                            <span class="sr-only">Loading...</span>
                                        </div>
                                    </button>
                                }
                                else
                                {
                                    <button id="signUp-panel" class="btn w-75" type="submit">Sign up!</button>
                                }
                            </div>
                        </EditForm>
                    </div>
                </div>
                <div class="form-container sign-in-container">
                    <div class="sign-in-form">
                        <EditForm Model="Model" OnValidSubmit="SignInSubmit">
                            <DataAnnotationsValidator />
                            <ValidationSummary Model="Model" />
                            <h1 class="text-center">Login</h1>
                            <div class="w-75 m-auto">
                                <input type="email" @bind-value="Model.Email" placeholder="Email" />
                                <input type="password" @bind-value="Model.Password" placeholder="Password" />
                            </div>
                            <div>
                                <p>@SignInMessage</p>
                            </div>
                            <div class="m-auto text-center">
                                <a href="passwordrecovery">Forgot password?</a>
                            </div>
                            <div class="w-75 m-auto">
                                @if (Disabled)
                                {
                                    <button id="signIn-panel" class="btn btn-block btn-primary w-75" disabled="@Disabled" type="submit">
                                        <div class="spinner-border" role="status">
                                            <span class="sr-only">Loading...</span>
                                        </div>
                                    </button>
                                }
                                else
                                {
                                    <button id="signIn-panel" class="btn w-75" type="submit">Log in!</button>
                                }
                            </div>
                        </EditForm>
